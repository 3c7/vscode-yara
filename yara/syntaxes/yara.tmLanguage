<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>fileTypes</key>
	<array>
		<string>yara</string>
		<string>yar</string>
	</array>
	<key>name</key>
	<string>YARA</string>
	<key>patterns</key>
	<array>
		<dict>
			<!-- block and line comments -->
			<key>include</key>
			<string>#comment</string>
		</dict>
		<dict>
			<!-- true/false -->
			<key>include</key>
			<string>#constant</string>
		</dict>
		<dict>
			<!-- general sections of the document -->
			<key>include</key>
			<string>#entity</key>
		</dict>
		<dict>
			<!-- miscellaneous -->
			<key>include</key>
			<string>#keyword</string>
		</dict>
		<dict>
			<!-- words that start with '$' -->
			<key>include</key>
			<string>#variable</string>
		</dict>
		<dict>
			<!-- YARA meta keywords -->
			<key>include</key>
			<string>#meta</string>
		</dict>
		<dict>
			<!-- strings, hex, regex -->
			<key>include</key>
			<string>#strings</string>
		</dict>
		<dict>
			<!-- invalid/deprecated -->
			<key>include</key>
			<string>#invalid</string>
		</dict>
	</array>
	<key>repository</key>
	<dict>
		<key>comment</key>
		<dict>
		</dict>
		<key>constant</key>
		<dict>
		</dict>
		<key>entity</key>
		<dict>
		</dict>
		<key>keyword</key>
		<dict>
		</dict>
		<key>variable</key>
		<dict>
		</dict>
		<key>meta</key>
		<dict>
		</dict>
		<key>strings</key>
		<dict>
		</dict>
		<key>invalid</key>
		<dict>
		</dict>
	</dict>
	<key>smartTypingPairs</key>
	<array>
		<array>
			<string>'</string>
			<string>'</string>
		</array>
		<array>
			<string>"</string>
			<string>"</string>
		</array>
		<array>
			<string>(</string>
			<string>)</string>
		</array>
		<array>
			<string>{</string>
			<string>}</string>
		</array>
		<array>
			<string>[</string>
			<string>]</string>
		</array>
		<array>
			<string>/*</string>
			<string>*/</string>
		</array>
	</array>
</dict>
</plist>