{
    "name": "yara",
    "displayName": "YARA Syntax Colorizer",
    "description": "Syntax Highlighting for the YARA pattern matching language",
    "version": "1.0.0",
    "icon": "images/logo.png",
    "galleryBanner": {
        "theme": "dark"
    },
    "publisher": "infosec-intern",
    "license": "MIT License - full doc in LICENSE.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/infosec-intern/textmate-yara"
    },
    "bugs": {
        "url": "https://github.com/infosec-intern/textmate-yara/issues"
    },
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "yara",
            "aliases": ["yara", "yara-syntax", "yaralang", "yara", "YARA", "Yara"],
            "extensions": [".yara",".yar"],
            "configuration": "yara/yara.configuration.json"
        }],
        "grammars": [{
            "language": "yara",
            "scopeName": "source.yara",
            "path": "yara/syntaxes/yara.tmLanguage"
        }]
    }
}